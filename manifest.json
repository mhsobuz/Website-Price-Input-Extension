{
  "manifest_version": 3,
  "name": "SKU Price Scraper",
  "version": "1.1",
  "permissions": ["tabs", "storage", "downloads"],
  "host_permissions": ["https://starlightlighting.ca/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["https://starlightlighting.ca/*"],
    "js": ["content.js"]
  }]
}
